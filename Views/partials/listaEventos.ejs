<style>
    .container {
        width: 100%;
    }

    .vertical-center {
        min-height: 100%;
        /* Fallback for browsers do NOT support vh unit */
        min-height: 100;
        /* These two lines are counted as one :-)       */
        display: flex;
        align-items: center;
    }

</style>
<br/><br/><br/><br/>
<div class="container text-center">
    <div class="row">
        <div class="col-md-2 alert alert-warning">Poster</div>
        <div class="col-md-2 alert alert-info">Event ID</div>
        <div class="col-md-2 alert alert-warning">Evento</div>
        <div class="col-md-2 alert alert-danger">Costo</div>
        <div class="col-md-2 alert alert-success">Fecha</div>
        <div class="col-md-2 alert alert-info ">Lugar</div>

    </div>
    <% contactos.forEach(function(contacto){%>
        <div class="row text-center ">
            <% var obj = JSON.parse( JSON.stringify(contacto)); %>

                <div class="col-md-2 table-bordered">
                    <img class='img-fluid' width="100" height="100" src='img/eventos/<%=obj.image %>' />
                </div>
                <div style="height:100px;" class="col-md-2 table-bordered">
                    <%=obj._id %>
                </div>
                <div style="height:100px;" class="col-md-2 table-bordered">
                    <%=obj.name %>
                </div>
                <div style="height:100px;" class="col-md-2 table-bordered">
                    <%=obj.cost %>
                </div>
                <div style="height:100px;" class="col-md-2 table-bordered">
                    <%=obj.date %>
                </div>
                <div style="height:100px;" class="col-md-2 table-bordered">
                    <%=obj.place %>
                </div>

        </div>

        <% });%>
</div>
<br/><br/><br/><br/>
<script src="js/jquery.js "></script>
